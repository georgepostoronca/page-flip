n}\n\n.stf__innerShadow {\n  position: absolute;\n  left: 0;\n  top: 0;\n}\n\n.stf__hardShadow {\n  position: absolute;\n  left: 0;\n  top: 0;\n}\n\n.stf__hardInnerShadow {\n  position: absolute;\n  left: 0;\n  top: 0;\n}\n\n\n.flip-book {\n  box-shadow: 0 0 20px 0 rgba(0, 0, 0, 0.5);\n  // display: none;\n  background-size: cover;\n}\n\n.page {\n  background-color: transparent;\n  color: rgb(120, 94, 58);\n\n  overflow: hidden;\n  box-sizing: border-box;\n\n  // & * {\n  //   border: 1px solid red;\n  // }\n\n  .page-content {\n    width: 100%;\n    height: 100%;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n    align-items: stretch;\n\n    img {\n      width: 100%;\n      height: 100%;\n      object-fit: cover;\n    }\n  }\n\n  &.--left { // for left page (property will be added automatically)\n    border-right: 0;\n    box-shadow: inset -7px 0 30px -7px rgba(0, 0, 0, 0.4);\n  }\n\n  &.--right { // for right page (property will be added automatically)\n    border-left: 0;\n    box-shadow: inset 7px 0 30px -7px rgba(0, 0, 0, 0.4);\n\n    .page-footer {\n      text-align: right;\n    }\n  }\n\n  &.hard { // for hard page\n    background-color: rgb(242, 232, 217);\n  }\n\n  &.page-cover {\n    background-color: rgb(223, 204, 177);\n    color:  rgb(120, 94, 58);\n\n    h2 {\n      text-align: center;\n      padding-top: 50%;\n      font-size: 210%;\n    }\n\n    &.page-cover-top {\n      box-shadow: inset 0px 0 30px 0px rgba(36, 10, 3, 0.5), -2px 0 5px 2px rgba(0, 0, 0, 0.4);\n    }\n\n    &.page-cover-bottom {\n      box-shadow: inset 0px 0 30px 0px rgba(36, 10, 3, 0.5), 10px 0 8px 0px rgba(0, 0, 0, 0.4);\n    }\n  }\n}\n\n// Container\n.container {\n  width: 1170px;\n  display: block;\n  margin: 0 auto;\n  box-sizing: border-box;\n\n  @media(max-width: 1170px) {\n    width: 100%;\n    padding-left: 15px;\n    padding-right: 15px;\n  }\n}\n\n// FlipBook\n.flipbook {\n  width: 100%;\n  height: 572px;\n  background-color: #262626;\n  position: relative;\n  overflow: hidden;\n\n  @media(max-width: 580px) {\n    height: 600px;\n  }\n\n  &__content {\n    height: calc(100% - 63px);\n    padding-left: 50px;\n    padding-right: 50px;\n\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    z-index: 99;\n\n    @media(max-width: 580px) {\n      padding-left: 15px;\n      padding-right: 15px;\n      height: calc(100% - 90px);\n    }\n\n    .flip-book {\n      width: 100%;\n      height: 100%;\n      box-shadow: none;\n    }\n  }\n\n  &__bottom {\n    height: 63px;\n    border-top: 1px solid #323a3d;\n    width: 100%;\n\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    box-sizing: border-box;\n\n    @media(max-width: 580px) {\n      position: relative;\n      height: 90px;\n      padding-top: 20px;\n      padding-bottom: 20px;\n    }\n  }\n\n  &__info {\n    font-family: \"Roboto-regular\", sans-serif;\n    color: #ffffff;\n    font-size: 14px;\n  }\n\n  &__prev, &__next {\n    width: 50px;\n    height: 100%;\n    position: absolute;\n    top: 0;\n    background: #323a3d;\n    cursor: pointer;\n\n    @media(max-width: 580px) {\n      top: 20px;\n      height: 50px;\n      border: 1px solid #707070;\n      background-color: transparent;\n    }\n\n    &:after {\n      content: \"\";\n      width: 12px;\n      height: 12px;\n      position: absolute;\n      top: 50%;\n      left: 50%;\n      transform: translate(-50%, -50%) rotate(45deg);\n      border: 2px solid #778a91;\n\n      @media(max-width: 580px) {\n        border-color: #707070;\n      }\n    }\n  }\n\n  &__prev {\n    left: 0;\n\n    @media(max-width: 580px) {\n      left: 15px;\n    }\n\n    &:after {\n      border-right: 0;\n      border-top: 0;\n      margin-left: 5px;\n\n      @media(max-width: 580px) {\n        margin-left: 3px;\n      }\n    }\n  }\n\n  &__next {\n    right: 0;\n\n    @media(max-width: 580px) {\n      right: 15px;\n    }\n\n    &:after {\n      border-left: 0;\n      border-bottom: 0;\n      margin-left: -5px;\n\n      @media(max-width: 580px) {\n        margin-left: -3px;\n      }\n    }\n  }\n\n  &__fullscreen {\n    padding: 0;\n    margin: 0;\n    background-color: transparent;\n    border: 0;\n    appearance: none;\n    box-shadow: none;\n\n    position: absolute;\n\n    right: calc(370px / 2);\n\n    @media(max-width: 1170px) {\n      right: 70px;\n    }\n\n    @media(max-width: 580px) {\n      display: none;\n    }\n\n    img {\n      display: block;\n    }\n  }\n}\n\n// Turn\n#flipbook-js {\n  width: 100%;\n  height: 100%;\n}\n\n.turn-page-wrapper {\n  img {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n  }\n}"},"lineCount":null}},"error":null,"hash":"ed3df704bfb4155af5a021cc5860d828","cacheData":{"env":{}}}